<!DOCTYPE html>
<html>
<head>
  <title></title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css">
  <link rel="stylesheet" type="text/css" href="css/styles.css">
  <link href='https://api.mapbox.com/mapbox.js/v2.3.0/mapbox.css' rel='stylesheet' />
</head>
<body>
  <nav>
    <div class="nav-wrapper red accent-4">
      <div class="container">
        <a href="/" class="brand-logo">RU Review</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
          <li><a href="/login">Login</a></li>
          <li><a href="/signup">Signup</a></li>
        </ul>
      </div>
    </div>
  </nav>

  <main>
    <div class="parallax-container">
      <div class="parallax"><img src="/images/collegeAve.jpg"></div>
    </div>

    <div class="section white">
      <div class="row container">
         <div class="col s12 m6" id="test">
          <div id="filter">
            <form action="#">
            <!--DINING-->
              <p>
                <input type="checkbox" id="dining" checked="checked"  />
                <label for="dining">Dining</label>
              </p>
              <!--RU SPORTS-->
              <p>
                <input type="checkbox" id="ru_sports" />
                <label for="ru_sports">RU Sports</label>
              </p>
              <!--ON-CAMPUS EVENTS-->
              <p>
                <input type="checkbox" id="campus_events" />
                <label for="campus_events">On-Campus Events</label>
              </p>
              <!--RECREATION-->
              <p>
                <input type="checkbox" id="recreation" />
                <label for="recreation">Things-to-do</label>
              </p>
            </form>
          </div>
        </div>
        <div class="parallax-container">
         <div id='map'></div>
        </div>

      </div>
    </div>
  </main>

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js"></script>
  <script type="text/javascript" src="js/scripts.js"></script>
  <!-- plugin for jQuery form validation -->
  <script src="https://cdn.jsdelivr.net/jquery.validation/1.15.0/jquery.validate.min.js"></script>
  <!-- js file for form validation -->
  <script type="text/javascript" src="js/formValidate.js"></script>
  <script src='https://api.mapbox.com/mapbox.js/v2.3.0/mapbox.js'></script>
</body>
</html>
