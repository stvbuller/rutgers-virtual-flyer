<h1>Users</h1>

{{#each users}}
  <div class="row">
    <div class="col-md-12">
      <p>{{firstname}}</p>
      <p>{{lastname}}</p>
      <p>{{username}}</p>
    </div>
  </div>
{{/each}}

<div class="row">
      {{#each reviews}}
        <div class="row">
          <div class="col-md-12">
            <p>{{locationName}}</p>
            <p>{{city}}</p>
            <p>{{rating}}</p>
            <p>{{review}}</p>
          </div>
        </div>
      {{/each}}
    </div>